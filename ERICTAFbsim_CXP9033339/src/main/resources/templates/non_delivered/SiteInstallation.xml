	<InstallationData logicalName="%Node Name%" vlanId="%vlan Id%" rbsIntegrationCode="%rbsIntegrationCode%">
		<SmrsData address="%SMRS Data address%" summaryFilePath="%Summary File Path%"/>
	</InstallationData>
